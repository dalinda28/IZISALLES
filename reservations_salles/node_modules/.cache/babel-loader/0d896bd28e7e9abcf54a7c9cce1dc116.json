Help\" placeholder=\"Votre email\">\n            </div>\n    \n            <!-- Message body -->\n            <div class=\"form-group\">\n              <label for=\"exampleFormControlTextarea1\">Message</label>\n              <textarea class=\"form-control\" id=\"exampleFormControlTextarea1\" rows=\"3\" placeholder=\"Votre message\"></textarea>\n            </div>\n    \n            <!-- Form actions -->\n            <div class=\"form-group\">\n              <div class=\"col-md-12 text-right\">\n                <button type=\"submit\" class=\"btn btn-secondary btn\" @click=\"showMsg=true\">Envoyer</button>\n              </div>\n            </div>\n          </fieldset>\n          </div>\n        </div>\n      </div>\n      <div id=\"overlay\" v-if=\"showMsg\">\n            <div class=\"modal-dialog\">\n                <div class=\"modal-content\">\n                    <div class=\"modal-body\">\n                        <p class=\"modal-title\">Votre message a été envoyée avec succès</p>\n                    </div>\n                    <div class=\"modal-footer\">\n                      <button type=\"button\" class=\"btn btn-secondary\" @click=\"refreshPage()\" data-dismiss=\"modal\">OK</button>\n                    </div>\n                </div>\n            </div>\n        </div>\n\t</div>\n</div>\n\n</template>\n\n<script>\nexport default {\n  data(){\n      return {\n          showMsg: false,\n      }\n    },\n    methods:{\n      //rafraîchissement de la page\n      refreshPage() {\n          this.showMsg = false;\n          this.$router.push('/');\n      },\n    }\n}\n</script>\n\n<style scoped>\n #overlay{\n        position: fixed;\n        top: 0;\n        bottom: 0;\n        left: 0;\n        right: 0;\n        background: rgba(0, 0, 0, 0.6);\n  }\n.container{\n    width: 70%;\n    margin-top: 10%;\n}\n.modal-dialog{\n    top:40%\n}\nbody{\n    padding-top: 5%;\n    background: rgb(245,245,245);\n}\n.row{\n    background: white;\n    border-radius: 30px;\n